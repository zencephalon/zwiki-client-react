{
  "parser": "babel-eslint",
  "globals": {
    // Put things like jQuery, etc
    "jQuery": true,
    "$": true
  },
  "extends": "airbnb",
  "rules": {
    "no-multiple-empty-lines": "off",
    "semi": ["error", "never"],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "new-cap": 0,
    "no-nested-ternary": 0
  }
}
